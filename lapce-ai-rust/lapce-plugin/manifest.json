{
  "name": "lapce-ai-assistant",
  "version": "1.0.0",
  "display_name": "AI Assistant (Ultra-fast)",
  "description": "World's fastest AI code assistant - 0.091Î¼s latency, 55M msg/sec",
  "author": "lapce-ai-rust",
  "icon": "icon.png",
  "engines": {
    "lapce": ">=0.2.0"
  },
  "main": "./dist/index.js",
  "contributes": {
    "commands": [
      {
        "command": "ai.complete",
        "title": "AI: Complete Code",
        "key": "ctrl+space"
      },
      {
        "command": "ai.explain",
        "title": "AI: Explain Code",
        "key": "ctrl+shift+e"
      },
      {
        "command": "ai.fix",
        "title": "AI: Fix Error",
        "key": "ctrl+shift+f"
      },
      {
        "command": "ai.refactor",
        "title": "AI: Refactor",
        "key": "ctrl+shift+r"
      }
    ],
    "keybindings": [
      {
        "key": "tab",
        "command": "ai.acceptSuggestion",
        "when": "suggestWidgetVisible"
      }
    ],
    "configuration": {
      "title": "AI Assistant",
      "properties": {
        "ai.enableAutoComplete": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI auto-completion"
        },
        "ai.latencyTarget": {
          "type": "number",
          "default": 10,
          "description": "Target latency in microseconds"
        },
        "ai.model": {
          "type": "string",
          "default": "fast",
          "enum": ["fast", "balanced", "quality"],
          "description": "AI model preference"
        }
      }
    }
  }
}
